#!/usr/bin/env php
<?php

require_once dirname(__DIR__) . '/vendor/autoload.php';

use Pac\Command;

$args = Command::config(['auto' => true])
    ->addOption('no', [
        'alias' => 'n',
        'type' => 'number',
    ])
    ->parseCommand($argv);

var_dump($args['invalids']);
var_dump($args);
exit;
