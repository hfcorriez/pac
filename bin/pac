#!/usr/bin/env php
<?php

require_once dirname(__DIR__) . '/vendor/autoload.php';

use Pac\Command;
use Pac\Console;

$args = Command::config(['auto' => true])
    ->addOption('no', [
        'alias' => 'n',
        'type' => 'number',
    ])
    ->parseCommand($argv);

Console::log('hello');
$password = Console::prompt('Plz input password: ', true);
var_dump($password);
Console::clip($password);
exit;
